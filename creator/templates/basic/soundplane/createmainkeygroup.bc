declare KEYGROUP,SCALE=major,TONIC=noted,OCTAVE=3,DEVICE=t3d device
# creates a keygroup and links it to device
# KEYGROUP=%KEYGROUP% keygroup to create

keygroup create 
it to %KEYGROUP% name ify
%KEYGROUP% hey scale to %SCALE% set
%KEYGROUP% hey tonic to %TONIC% set
%KEYGROUP% hey octave to %OCTAVE% set
%DEVICE% to %KEYGROUP% connect
%DEVICE% to %KEYGROUP% mode key input connect


